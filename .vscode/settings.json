{
  "files.exclude": {
    "tmp": true,
    "coverage_test.out": true,
    "coverage.out": true
  },
  "go.lintOnSave": "workspace",
  "go.formatTool": "goimports",
  "editor.formatOnSave": true,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "gopls": {
    "ui.diagnostic.staticcheck": true,
    "ui.diagnostic.analyses": {
      "unusedparams": true,
      "shadow": true,
      "unusedwrite": true,
      "useany": true,
      "unusedvariable": true
    }
  }
}